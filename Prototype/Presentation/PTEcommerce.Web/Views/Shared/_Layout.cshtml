@{
    var controller = ViewContext.RouteData.Values["Controller"].ToString().ToLower();
    var mems = PTEcommerce.Web.Models.SessionCustomer.GetUser();
    var mobile = Request.Browser.IsMobileDevice;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta property="og:image" content="./publics/images/logo.png" />
    <meta property="og:image:type" content="image/jpeg" />
    <title>MVC</title>

    <!-- BOOTSTRAP -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">

    <!-- SWIPPER -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.css" />

    <!-- manifest meta -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <link rel="manifest" href="manifest.json" />

    <!-- Favicons -->
    <link rel="apple-touch-icon" href="~/Assets/assets/images/loading.png" sizes="180x180">
    <link rel="icon" href="~/Assets/assets/images/loading.png" sizes="32x32" type="image/png">
    <link rel="icon" href="~/Assets/assets/images/loading.png" sizes="16x16" type="image/png">

    <!-- Google fonts-->
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;700&display=swap" rel="stylesheet">

    <!-- bootstrap icons -->
    <link href="~/Assets/bootstrap-icons/font/bootstrap-icons.css" rel="stylesheet" />

    <!-- nouislider CSS -->
    <link href="~/Assets/assets/vendor/nouislider/nouislider.min.css" rel="stylesheet">
    <link href="~/Assets/toastr/toastr.css" rel="stylesheet" />

    <!-- swiper css -->
    <link rel="stylesheet" href="~/Assets/assets/vendor/swiperjs-6.6.2/swiper-bundle.min.css">

    <!-- style css for this template -->
    <link href="~/Assets/assets/css/style.css?v=@DateTime.Now.ToString("yyyyMMddHHmmss")" rel="stylesheet" id="style">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.0/jquery.min.js" integrity="sha512-3gJwYpMe3QewGELv8k/BX9vcqhryRdzRMxVfq6ngyWXwo03GFEzjsUm8Q7RZcHPHksttq7/GFoxjCVUjkjvPdw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

    <!-- BASE CSS -->
    <link rel="stylesheet" href="~/Assets/publics/css/style.css">
</head>

<body>
    <div class="work-page home">
        <!-- NAVBAR START -->
        <div class="navbar">
            <button class="btn-nav">
                <div class="btn-nav col-1">
                    <div class="item-btn top"></div>
                    <div class="item-btn center"></div>
                    <div class="item-btn bottom"></div>
                </div>
            </button>
            <div class="noti-navbar col-3">
                <a href="#">
                    <img src="~/Assets/publics/images/logo.png" width="100%" alt="">
                </a>
            </div>
        </div>

        <div class="left-bar">
            <div class="background-left-bar"></div>
            <div class="content-left-bar">
                <div class="info-user">
                    <div class="col-4 avatar">
                        <img src="~/Assets/publics/images/avata.png" width="100%" alt="">
                    </div>
                    <div class="col-8 user-name">
                        <b>
                            <a href="/thong-tin-ca-nhan"> @mems.Username</a>
                        </b>
                        <p class="text-opac">Số dư: <span class="moneyReload"></span></p>
                    </div>
                </div>
                <hr>
                <div class="menu">
                    <div class="item-menu">
                        <a href="/">
                            <div class="icon">
                                <img src="~/Assets/publics/images/btn-home.png" width="100%" alt="">
                            </div>
                            <b>Trang chủ</b>
                        </a>
                    </div>
                    @if (mems != null)
                    {
                        <div class="item-menu">
                            <a href="/thong-tin-ca-nhan">
                                <div class="icon">
                                    <img src="~/Assets/publics/images/btn-info.png" width="100%" alt="">
                                </div>
                                <b>Thông tin cá nhân</b>
                            </a>
                        </div>
                        <div class="item-menu">
                            <a href="/don-hang-cua-toi">
                                <div class="icon">
                                    <img src="~/Assets/publics/images/btn-order.png" width="100%" alt="">
                                </div>
                                <b>Đơn hàng của tôi</b>
                            </a>
                        </div>
                        <div class="item-menu">
                            <a href="/rut-tien">
                                <div class="icon">
                                    <img src="~/Assets/publics/images/btn-payment.png" width="100%" alt="">
                                </div>
                                <b>Rút tiền</b>
                            </a>
                        </div>
                        <div class="item-menu">
                            <a href="/lich-su-rut-tien">
                                <div class="icon">
                                    <img src="~/Assets/publics/images/btn-history-payment.png" width="100%" alt="">
                                </div>
                                <b>Lịch sử rút tiền</b>
                            </a>
                        </div>
                        <div class="item-menu">
                            <a href="/dang-xuat">
                                <div class="icon">
                                    <img src="~/Assets/publics/images/btn-logout.png" width="100%" alt="">
                                </div>
                                <b>Đăng xuất</b>
                            </a>
                        </div>
                    }
                </div>
            </div>
        </div>
        <!-- NAVBAR END -->
        <!-- NAV BOTTOM START -->

        <div class="nav-bottom">
            <p>Copyright  c  2023 MCV Group, All Rights Reserved</p>
        </div>

        <!-- NAV BOTTOM END -->
        <!-- MAIN START -->
        <div class="main">
            @RenderBody()
        </div>
        <!-- MAIN END -->
    </div>
    <!-- JQUERY -->
    

    <!-- SWIPPER -->
    <script src="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.js"></script>
    <script src="~/Assets/assets/js/jquery.cookie.js"></script>
    <script src="~/Assets/toastr/toastr.js"></script>
    <script src="~/Assets/controller/base.js?v=@DateTime.Now.ToString("yyyyMMddHHmmss")"></script>
    <script src="~/Assets/controller/accounting.js"></script>
    <script src="~/Assets/controller/@(controller).js?v=@DateTime.Now.ToString("yyyyMMddHHmmss")"></script>
    <script>
        var swiper = new Swiper(".donate", {
            slidesPerView: 1,
            spaceBetween: 35,
            loop: true,
            autoplay: {
                delay: 2500,
                disableOnInteraction: false,
            },
            navigation: {
                nextEl: ".swiper-button-next",
                prevEl: ".swiper-button-prev",
            },
        });

        // SLIDER
        var swiper = new Swiper(".banner", {
            loop: true,
            autoplay: {
                delay: 2500,
                disableOnInteraction: false,
            },
            pagination: {
                el: ".swiper-pagination",
                clickable: true,
            }
        });

        // NAV LEFT START
        $(document).ready(function () {

            $(".btn-nav").click(function () {
                $(".left-bar").addClass("active");
                $("body").addClass("active");
            });

            $(".background-left-bar").click(function () {
                $(".left-bar").removeClass("active");
                $("body").removeClass("active");
            });
        });

        $('.btn-item').click(function () {
            console.log(123);
            $('.btn-item').removeClass('active');
            $(this).addClass('active');
        });

    </script>

</body>

</html>